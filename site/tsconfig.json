{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "isolatedModules": false,
    "declaration": true,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "noEmitOnError": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "inlineSourceMap": true
  },
  "include": [
    "*.ts",
    "../typings/vega.d.ts",
    "../typings/vega-util.d.ts",
    "../typings/vega-embed.d.ts"
  ]
}
