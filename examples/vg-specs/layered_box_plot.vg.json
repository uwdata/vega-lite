{
    "$schema": "http://vega.github.io/schema/vega/v3.0.json",
    "description": "A horizontal box plot showing median, min, and max in the US population distribution of age groups in 2000.",
    "autosize": {
        "type": "pad",
        "resize": true
    },
    "padding": 5,
    "data": [
        {
            "name": "source_0",
            "url": "data/population.json",
            "format": {
                "type": "json"
            },
            "transform": [
                {
                    "type": "aggregate",
                    "groupby": [
                        "age"
                    ],
                    "ops": [
                        "min",
                        "q1",
                        "q3",
                        "max",
                        "median"
                    ],
                    "fields": [
                        "people",
                        "people",
                        "people",
                        "people",
                        "people"
                    ],
                    "as": [
                        "people_min",
                        "people_q1",
                        "people_q3",
                        "people_max",
                        "people_median"
                    ]
                }
            ]
        },
        {
            "name": "data_0",
            "source": "source_0",
            "transform": [
                {
                    "type": "formula",
                    "expr": "toNumber(datum[\"people_min\"])",
                    "as": "people_min"
                },
                {
                    "type": "formula",
                    "expr": "toNumber(datum[\"people_q1\"])",
                    "as": "people_q1"
                },
                {
                    "type": "filter",
                    "expr": "datum[\"people_min\"] !== null && !isNaN(datum[\"people_min\"]) && datum[\"people_q1\"] !== null && !isNaN(datum[\"people_q1\"])"
                }
            ]
        },
        {
            "name": "data_1",
            "source": "source_0",
            "transform": [
                {
                    "type": "formula",
                    "expr": "toNumber(datum[\"people_q3\"])",
                    "as": "people_q3"
                },
                {
                    "type": "formula",
                    "expr": "toNumber(datum[\"people_max\"])",
                    "as": "people_max"
                },
                {
                    "type": "filter",
                    "expr": "datum[\"people_q3\"] !== null && !isNaN(datum[\"people_q3\"]) && datum[\"people_max\"] !== null && !isNaN(datum[\"people_max\"])"
                }
            ]
        },
        {
            "name": "data_2",
            "source": "source_0",
            "transform": [
                {
                    "type": "formula",
                    "expr": "toNumber(datum[\"people_q1\"])",
                    "as": "people_q1"
                },
                {
                    "type": "formula",
                    "expr": "toNumber(datum[\"people_q3\"])",
                    "as": "people_q3"
                },
                {
                    "type": "filter",
                    "expr": "datum[\"people_q1\"] !== null && !isNaN(datum[\"people_q1\"]) && datum[\"people_q3\"] !== null && !isNaN(datum[\"people_q3\"])"
                }
            ]
        },
        {
            "name": "data_3",
            "source": "source_0",
            "transform": [
                {
                    "type": "formula",
                    "expr": "toNumber(datum[\"people_median\"])",
                    "as": "people_median"
                },
                {
                    "type": "filter",
                    "expr": "datum[\"people_median\"] !== null && !isNaN(datum[\"people_median\"])"
                }
            ]
        }
    ],
    "signals": [
        {
            "name": "width",
            "update": "max(layer_0_width, layer_1_width, layer_2_width, layer_3_width)"
        },
        {
            "name": "height",
            "update": "max(layer_0_height, layer_1_height, layer_2_height, layer_3_height)"
        },
        {
            "name": "layer_0_width",
            "update": "200"
        },
        {
            "name": "layer_0_height",
            "update": "bandspace(domain('y').length, 1, 0.5) * 21"
        },
        {
            "name": "layer_1_width",
            "update": "200"
        },
        {
            "name": "layer_1_height",
            "update": "bandspace(domain('y').length, 1, 0.5) * 21"
        },
        {
            "name": "layer_2_width",
            "update": "200"
        },
        {
            "name": "layer_2_height",
            "update": "bandspace(domain('y').length, 1, 0.5) * 21"
        },
        {
            "name": "layer_3_width",
            "update": "200"
        },
        {
            "name": "layer_3_height",
            "update": "bandspace(domain('y').length, 1, 0.5) * 21"
        }
    ],
    "marks": [
        {
            "name": "nested_main_group",
            "type": "group",
            "encode": {
                "update": {
                    "width": {
                        "signal": "width"
                    },
                    "height": {
                        "signal": "height"
                    },
                    "fill": {
                        "value": "transparent"
                    }
                }
            },
            "marks": [
                {
                    "name": "layer_0_marks",
                    "type": "rule",
                    "role": "boxWhisker",
                    "from": {
                        "data": "data_0"
                    },
                    "encode": {
                        "update": {
                            "x": {
                                "scale": "x",
                                "field": "people_min"
                            },
                            "y": {
                                "scale": "y",
                                "field": "age"
                            },
                            "x2": {
                                "scale": "x",
                                "field": "people_q1"
                            },
                            "stroke": {
                                "value": "#4c78a8"
                            }
                        }
                    },
                    "clip": true
                },
                {
                    "name": "layer_1_marks",
                    "type": "rule",
                    "role": "boxWhisker",
                    "from": {
                        "data": "data_1"
                    },
                    "encode": {
                        "update": {
                            "x": {
                                "scale": "x",
                                "field": "people_q3"
                            },
                            "y": {
                                "scale": "y",
                                "field": "age"
                            },
                            "x2": {
                                "scale": "x",
                                "field": "people_max"
                            },
                            "stroke": {
                                "value": "#4c78a8"
                            }
                        }
                    },
                    "clip": true
                },
                {
                    "name": "layer_2_marks",
                    "type": "rect",
                    "role": "box",
                    "from": {
                        "data": "data_2"
                    },
                    "encode": {
                        "update": {
                            "x": {
                                "scale": "x",
                                "field": "people_q1"
                            },
                            "x2": {
                                "scale": "x",
                                "field": "people_q3"
                            },
                            "yc": {
                                "scale": "y",
                                "field": "age"
                            },
                            "height": {
                                "value": 5
                            },
                            "fill": {
                                "value": "skyblue"
                            }
                        }
                    },
                    "clip": true
                },
                {
                    "name": "layer_3_marks",
                    "type": "rect",
                    "role": "boxMid",
                    "from": {
                        "data": "data_3"
                    },
                    "encode": {
                        "update": {
                            "xc": {
                                "scale": "x",
                                "field": "people_median"
                            },
                            "yc": {
                                "scale": "y",
                                "field": "age"
                            },
                            "height": {
                                "value": 5
                            },
                            "width": {
                                "value": 1
                            },
                            "fill": {
                                "value": "white"
                            },
                            "opacity": {
                                "value": 0.7
                            }
                        }
                    },
                    "clip": true
                }
            ]
        }
    ],
    "scales": [
        {
            "name": "x",
            "type": "linear",
            "domain": {
                "fields": [
                    {
                        "data": "data_0",
                        "field": "people_min"
                    },
                    {
                        "data": "data_0",
                        "field": "people_q1"
                    },
                    {
                        "data": "data_1",
                        "field": "people_q3"
                    },
                    {
                        "data": "data_1",
                        "field": "people_max"
                    },
                    {
                        "data": "data_2",
                        "field": "people_q1"
                    },
                    {
                        "data": "data_2",
                        "field": "people_q3"
                    },
                    {
                        "data": "data_3",
                        "field": "people_median"
                    }
                ],
                "sort": true
            },
            "range": [
                0,
                200
            ],
            "round": true,
            "nice": true,
            "zero": true
        },
        {
            "name": "y",
            "type": "point",
            "domain": {
                "fields": [
                    {
                        "data": "data_0",
                        "field": "age"
                    },
                    {
                        "data": "data_1",
                        "field": "age"
                    },
                    {
                        "data": "data_2",
                        "field": "age"
                    },
                    {
                        "data": "data_3",
                        "field": "age"
                    }
                ],
                "sort": true
            },
            "range": {
                "step": 21
            },
            "round": true,
            "padding": 0.5
        }
    ],
    "axes": [
        {
            "scale": "x",
            "format": "s",
            "orient": "bottom",
            "tickCount": 5,
            "title": "population",
            "zindex": 1
        },
        {
            "scale": "x",
            "domain": false,
            "format": "s",
            "grid": true,
            "labels": false,
            "orient": "bottom",
            "tickCount": 5,
            "ticks": false,
            "zindex": 0,
            "gridScale": "y"
        },
        {
            "scale": "y",
            "orient": "left",
            "title": "age",
            "zindex": 1
        }
    ]
}