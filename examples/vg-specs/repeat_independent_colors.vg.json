                },
                {
                    "scale": "child_Cylinders_y",
                    "orient": "left",
                    "title": "Miles_per_Gallon",
                    "zindex": 1
                },
                {
                    "scale": "child_Cylinders_y",
                    "domain": false,
                    "grid": true,
                    "labels": false,
                    "orient": "left",
                    "ticks": false,
                    "zindex": 0,
                    "gridScale": "child_Cylinders_x"
                }
            ],
            "legends": [
