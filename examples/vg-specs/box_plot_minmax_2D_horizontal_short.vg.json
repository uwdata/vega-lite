{
    "$schema": "http://vega.github.io/schema/vega/v3.0.json",
    "description": "A horizontal box plot showing median, min, and max in the US population distribution of age groups in 2000.",
    "autosize": "pad",
    "padding": 5,
    "data": [
        {
            "name": "source_0",
            "url": "data/population.json",
            "format": {
                "type": "json"
            }
        },
        {
            "name": "data_0",
            "source": "source_0",
            "transform": [
                {
                    "type": "formula",
                    "expr": "toNumber(datum[\"people\"])",
                    "as": "people"
                },
                {
                    "type": "filter",
                    "expr": "datum[\"people\"] !== null && !isNaN(datum[\"people\"])"
                },
                {
                    "type": "aggregate",
                    "groupby": [
                        "age"
                    ],
                    "ops": [
                        "min",
                        "q1"
                    ],
                    "fields": [
                        "people",
                        "people"
                    ]
                }
            ]
        },
        {
            "name": "data_1",
            "source": "source_0",
            "transform": [
                {
                    "type": "formula",
                    "expr": "toNumber(datum[\"people\"])",
                    "as": "people"
                },
                {
                    "type": "filter",
                    "expr": "datum[\"people\"] !== null && !isNaN(datum[\"people\"])"
                },
                {
                    "type": "aggregate",
                    "groupby": [
                        "age"
                    ],
                    "ops": [
                        "q3",
                        "max"
                    ],
                    "fields": [
                        "people",
                        "people"
                    ]
                }
            ]
        },
        {
            "name": "data_2",
            "source": "source_0",
            "transform": [
                {
                    "type": "formula",
                    "expr": "toNumber(datum[\"people\"])",
                    "as": "people"
                },
                {
                    "type": "filter",
                    "expr": "datum[\"people\"] !== null && !isNaN(datum[\"people\"])"
                },
                {
                    "type": "aggregate",
                    "groupby": [
                        "age"
                    ],
                    "ops": [
                        "q1",
                        "q3"
                    ],
                    "fields": [
                        "people",
                        "people"
                    ]
                }
            ]
        },
        {
            "name": "data_3",
            "source": "source_0",
            "transform": [
                {
                    "type": "formula",
                    "expr": "toNumber(datum[\"people\"])",
                    "as": "people"
                },
                {
                    "type": "filter",
                    "expr": "datum[\"people\"] !== null && !isNaN(datum[\"people\"])"
                },
                {
                    "type": "aggregate",
                    "groupby": [
                        "age"
                    ],
                    "ops": [
                        "median"
                    ],
                    "fields": [
                        "people"
                    ]
                }
            ]
        }
    ],
    "signals": [
        {
            "name": "width",
            "update": "max(layer_0_width, layer_1_width, layer_2_width, layer_3_width)"
        },
        {
            "name": "height",
            "update": "max(layer_0_height, layer_1_height, layer_2_height, layer_3_height)"
        },
        {
            "name": "layer_0_width",
            "update": "200"
        },
        {
            "name": "layer_0_height",
            "update": "bandspace(domain('y').length, 1, 0.5) * 21"
        },
        {
            "name": "layer_1_width",
            "update": "200"
        },
        {
            "name": "layer_1_height",
            "update": "bandspace(domain('y').length, 1, 0.5) * 21"
        },
        {
            "name": "layer_2_width",
            "update": "200"
        },
        {
            "name": "layer_2_height",
            "update": "bandspace(domain('y').length, 1, 0.5) * 21"
        },
        {
            "name": "layer_3_width",
            "update": "200"
        },
        {
            "name": "layer_3_height",
            "update": "bandspace(domain('y').length, 1, 0.5) * 21"
        },
        {
            "name": "unit",
            "value": {},
            "on": [
                {
                    "events": "mousemove",
                    "update": "group()._id ? group() : unit"
                }
            ]
        }
    ],
    "marks": [
        {
            "name": "nested_main_group",
            "type": "group",
            "encode": {
                "update": {
                    "width": {
                        "signal": "width"
                    },
                    "height": {
                        "signal": "height"
                    }
                }
            },
            "marks": [
                {
                    "type": "group",
                    "encode": {
                        "enter": {
                            "width": {
                                "signal": "width"
                            },
                            "height": {
                                "signal": "height"
                            },
                            "fill": {
                                "value": "transparent"
                            },
                            "clip": {
                                "value": true
                            }
                        }
                    },
                    "marks": [
                        {
                            "name": "layer_0_marks",
                            "type": "rule",
                            "role": "boxWhisker",
                            "from": {
                                "data": "data_0"
                            },
                            "encode": {
                                "update": {
                                    "x": {
                                        "scale": "x",
                                        "field": "min_people"
                                    },
                                    "y": {
                                        "scale": "y",
                                        "field": "age"
                                    },
                                    "x2": {
                                        "scale": "x",
                                        "field": "q1_people"
                                    },
                                    "stroke": {
                                        "value": "#4c78a8"
                                    }
                                }
                            }
                        },
                        {
                            "name": "layer_1_marks",
                            "type": "rule",
                            "role": "boxWhisker",
                            "from": {
                                "data": "data_1"
                            },
                            "encode": {
                                "update": {
                                    "x": {
                                        "scale": "x",
                                        "field": "q3_people"
                                    },
                                    "y": {
                                        "scale": "y",
                                        "field": "age"
                                    },
                                    "x2": {
                                        "scale": "x",
                                        "field": "max_people"
                                    },
                                    "stroke": {
                                        "value": "#4c78a8"
                                    }
                                }
                            }
                        },
                        {
                            "name": "layer_2_marks",
                            "type": "rect",
                            "role": "box",
                            "from": {
                                "data": "data_2"
                            },
                            "encode": {
                                "update": {
                                    "x": {
                                        "scale": "x",
                                        "field": "q1_people"
                                    },
                                    "x2": {
                                        "scale": "x",
                                        "field": "q3_people"
                                    },
                                    "yc": {
                                        "scale": "y",
                                        "field": "age"
                                    },
                                    "height": {
                                        "value": 5
                                    },
                                    "fill": {
                                        "value": "skyblue"
                                    }
                                }
                            }
                        },
                        {
                            "name": "layer_3_marks",
                            "type": "rect",
                            "role": "boxMid",
                            "from": {
                                "data": "data_3"
                            },
                            "encode": {
                                "update": {
                                    "xc": {
                                        "scale": "x",
                                        "field": "median_people"
                                    },
                                    "yc": {
                                        "scale": "y",
                                        "field": "age"
                                    },
                                    "height": {
                                        "value": 5
                                    },
                                    "width": {
                                        "value": 1
                                    },
                                    "fill": {
                                        "value": "white"
                                    }
                                }
                            }
                        }
                    ]
                }
            ]
        }
    ],
    "scales": [
        {
            "name": "x",
            "type": "linear",
            "domain": {
                "fields": [
                    {
                        "data": "data_0",
                        "field": "min_people"
                    },
                    {
                        "data": "data_0",
                        "field": "q1_people"
                    },
                    {
                        "data": "data_1",
                        "field": "q3_people"
                    },
                    {
                        "data": "data_1",
                        "field": "max_people"
                    },
                    {
                        "data": "data_2",
                        "field": "q1_people"
                    },
                    {
                        "data": "data_2",
                        "field": "q3_people"
                    },
                    {
                        "data": "data_3",
                        "field": "median_people"
                    }
                ],
                "sort": true
            },
            "range": [
                0,
                200
            ],
            "round": true,
            "nice": true,
            "zero": true
        },
        {
            "name": "y",
            "type": "point",
            "domain": {
                "fields": [
                    {
                        "data": "data_0",
                        "field": "age"
                    },
                    {
                        "data": "data_1",
                        "field": "age"
                    },
                    {
                        "data": "data_2",
                        "field": "age"
                    },
                    {
                        "data": "data_3",
                        "field": "age"
                    }
                ],
                "sort": true
            },
            "range": {
                "step": 21
            },
            "round": true,
            "padding": 0.5
        }
    ],
    "axes": [
        {
            "scale": "x",
            "format": "s",
            "orient": "bottom",
            "tickCount": 5,
            "title": "population",
            "zindex": 1
        },
        {
            "scale": "x",
            "domain": false,
            "format": "s",
            "grid": true,
            "labels": false,
            "orient": "bottom",
            "tickCount": 5,
            "ticks": false,
            "zindex": 0,
            "gridScale": "y"
        },
        {
            "scale": "y",
            "orient": "left",
            "title": "age",
            "zindex": 1
        }
    ]
}