{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "autosize": "pad",
  "padding": 5,
  "width": 200,
  "height": 200,
  "style": "cell",
  "data": [
    {
      "name": "source_0",
      "url": "data/cars.json",
      "format": {"type": "json", "parse": {"Year": "date"}},
      "transform": [
        {
          "type": "formula",
          "as": "year_Year",
          "expr": "datetime(year(datum[\"Year\"]), 0, 1, 0, 0, 0, 0)"
        },
        {
          "type": "aggregate",
          "groupby": ["year_Year"],
          "ops": ["ci0", "ci1", "mean"],
          "fields": [
            "Miles_per_Gallon",
            "Miles_per_Gallon",
            "Miles_per_Gallon"
          ],
          "as": [
            "lower_Miles_per_Gallon",
            "upper_Miles_per_Gallon",
            "center_Miles_per_Gallon"
          ]
        }
      ]
    }
  ],
  "marks": [
    {
      "name": "layer_0_marks",
      "type": "rect",
      "style": ["tick", "errorbar-ticks"],
      "from": {"data": "source_0"},
      "encode": {
        "update": {
          "opacity": {"value": 0.7},
          "fill": [
            {
              "test": "datum[\"year_Year\"] === null || isNaN(datum[\"year_Year\"]) || datum[\"lower_Miles_per_Gallon\"] === null || isNaN(datum[\"lower_Miles_per_Gallon\"])",
              "value": null
            },
            {"value": "#4c78a8"}
          ],
          "tooltip": {
            "signal": "{\"Ci1 of Miles_per_Gallon\": format(datum[\"upper_Miles_per_Gallon\"], \"\"), \"Ci0 of Miles_per_Gallon\": format(datum[\"lower_Miles_per_Gallon\"], \"\"), \"Mean of Miles_per_Gallon\": format(datum[\"center_Miles_per_Gallon\"], \"\"), \"Year (year)\": timeFormat(datum[\"year_Year\"], '%Y')}"
          },
          "xc": {"scale": "x", "field": "year_Year"},
          "yc": [
            {
              "test": "datum[\"lower_Miles_per_Gallon\"] === null || isNaN(datum[\"lower_Miles_per_Gallon\"])",
              "field": {"group": "height"}
            },
            {"scale": "y", "field": "lower_Miles_per_Gallon"}
          ],
          "width": {"value": 15},
          "height": {"value": 1}
        }
      }
    },
    {
      "name": "layer_1_marks",
      "type": "rect",
      "style": ["tick", "errorbar-ticks"],
      "from": {"data": "source_0"},
      "encode": {
        "update": {
          "opacity": {"value": 0.7},
          "fill": [
            {
              "test": "datum[\"year_Year\"] === null || isNaN(datum[\"year_Year\"]) || datum[\"upper_Miles_per_Gallon\"] === null || isNaN(datum[\"upper_Miles_per_Gallon\"])",
              "value": null
            },
            {"value": "#4c78a8"}
          ],
          "tooltip": {
            "signal": "{\"Ci1 of Miles_per_Gallon\": format(datum[\"upper_Miles_per_Gallon\"], \"\"), \"Ci0 of Miles_per_Gallon\": format(datum[\"lower_Miles_per_Gallon\"], \"\"), \"Mean of Miles_per_Gallon\": format(datum[\"center_Miles_per_Gallon\"], \"\"), \"Year (year)\": timeFormat(datum[\"year_Year\"], '%Y')}"
          },
          "xc": {"scale": "x", "field": "year_Year"},
          "yc": [
            {
              "test": "datum[\"upper_Miles_per_Gallon\"] === null || isNaN(datum[\"upper_Miles_per_Gallon\"])",
              "field": {"group": "height"}
            },
            {"scale": "y", "field": "upper_Miles_per_Gallon"}
          ],
          "width": {"value": 15},
          "height": {"value": 1}
        }
      }
    },
    {
      "name": "layer_2_marks",
      "type": "rule",
      "style": ["rule", "errorbar-rule"],
      "from": {"data": "source_0"},
      "encode": {
        "update": {
          "stroke": [
            {
              "test": "datum[\"year_Year\"] === null || isNaN(datum[\"year_Year\"]) || datum[\"lower_Miles_per_Gallon\"] === null || isNaN(datum[\"lower_Miles_per_Gallon\"])",
              "value": null
            },
            {"value": "black"}
          ],
          "tooltip": {
            "signal": "{\"Ci1 of Miles_per_Gallon\": format(datum[\"upper_Miles_per_Gallon\"], \"\"), \"Ci0 of Miles_per_Gallon\": format(datum[\"lower_Miles_per_Gallon\"], \"\"), \"Mean of Miles_per_Gallon\": format(datum[\"center_Miles_per_Gallon\"], \"\"), \"Year (year)\": timeFormat(datum[\"year_Year\"], '%Y')}"
          },
          "x": {"scale": "x", "field": "year_Year"},
          "y": [
            {
              "test": "datum[\"lower_Miles_per_Gallon\"] === null || isNaN(datum[\"lower_Miles_per_Gallon\"])",
              "field": {"group": "height"}
            },
            {"scale": "y", "field": "lower_Miles_per_Gallon"}
          ],
          "y2": [
            {
              "test": "datum[\"upper_Miles_per_Gallon\"] === null || isNaN(datum[\"upper_Miles_per_Gallon\"])",
              "field": {"group": "height"}
            },
            {"scale": "y", "field": "upper_Miles_per_Gallon"}
          ]
        }
      }
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "time",
      "domain": {"data": "source_0", "field": "year_Year"},
      "range": [0, {"signal": "width"}]
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "data": "source_0",
        "fields": ["lower_Miles_per_Gallon", "upper_Miles_per_Gallon"]
      },
      "range": [{"signal": "height"}, 0],
      "zero": false,
      "nice": true
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "grid": false,
      "title": "Year (year)",
      "labelFlush": true,
      "labelOverlap": true,
      "tickCount": {"signal": "ceil(width/40)"},
      "encode": {
        "labels": {
          "update": {"text": {"signal": "timeFormat(datum.value, '%Y')"}}
        }
      },
      "zindex": 1
    },
    {
      "scale": "x",
      "orient": "bottom",
      "gridScale": "y",
      "grid": true,
      "tickCount": {"signal": "ceil(width/40)"},
      "domain": false,
      "labels": false,
      "maxExtent": 0,
      "minExtent": 0,
      "ticks": false,
      "zindex": 0
    },
    {
      "scale": "y",
      "orient": "left",
      "grid": false,
      "title": "Miles per Gallon (95% CIs)",
      "labelOverlap": true,
      "tickCount": {"signal": "ceil(height/40)"},
      "zindex": 1
    },
    {
      "scale": "y",
      "orient": "left",
      "gridScale": "x",
      "grid": true,
      "tickCount": {"signal": "ceil(height/40)"},
      "domain": false,
      "labels": false,
      "maxExtent": 0,
      "minExtent": 0,
      "ticks": false,
      "zindex": 0
    }
  ],
  "config": {"axisY": {"minExtent": 30}}
}
