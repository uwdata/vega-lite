{
  "$schema": "https://vega.github.io/schema/vega/v4.json",
  "autosize": "pad",
  "padding": 5,
  "width": 200,
  "height": 200,
  "style": "cell",
  "data": [
    {
      "name": "source_0",
      "url": "data/cars.json",
      "format": {"type": "json"},
      "transform": [
        {
          "type": "formula",
          "as": "year_Year",
          "expr": "datetime(year(datum[\"Year\"]), 0, 1, 0, 0, 0, 0)"
        },
        {
          "type": "aggregate",
          "groupby": ["year_Year"],
          "ops": ["ci0", "ci1"],
          "fields": ["Miles_per_Gallon", "Miles_per_Gallon"],
          "as": ["lower_Miles_per_Gallon", "upper_Miles_per_Gallon"]
        }
      ]
    },
    {
      "name": "data_0",
      "source": "source_0",
      "transform": [
        {
          "type": "filter",
          "expr": "datum[\"lower_Miles_per_Gallon\"] !== null && !isNaN(datum[\"lower_Miles_per_Gallon\"]) && datum[\"year_Year\"] !== null && !isNaN(datum[\"year_Year\"])"
        }
      ]
    },
    {
      "name": "data_1",
      "source": "source_0",
      "transform": [
        {
          "type": "filter",
          "expr": "datum[\"upper_Miles_per_Gallon\"] !== null && !isNaN(datum[\"upper_Miles_per_Gallon\"]) && datum[\"year_Year\"] !== null && !isNaN(datum[\"year_Year\"])"
        }
      ]
    },
    {
      "name": "data_2",
      "source": "source_0",
      "transform": [
        {
          "type": "filter",
          "expr": "datum[\"lower_Miles_per_Gallon\"] !== null && !isNaN(datum[\"lower_Miles_per_Gallon\"]) && datum[\"year_Year\"] !== null && !isNaN(datum[\"year_Year\"])"
        }
      ]
    }
  ],
  "marks": [
    {
      "name": "layer_0_marks",
      "type": "rect",
      "style": ["tick", "errorbar-ticks"],
      "from": {"data": "data_0"},
      "encode": {
        "update": {
          "opacity": {"value": 0.7},
          "fill": {"value": "#4c78a8"},
          "tooltip": {
            "signal": "{\"Year (year)\": timeFormat(datum[\"year_Year\"], ''), \"Miles per Gallon (95% CIs)\": format(datum[\"lower_Miles_per_Gallon\"], \"\")}"
          },
          "xc": {"scale": "x", "field": "year_Year"},
          "yc": {"scale": "y", "field": "lower_Miles_per_Gallon"},
          "width": {"value": 14},
          "height": {"value": 1}
        }
      }
    },
    {
      "name": "layer_1_marks",
      "type": "rect",
      "style": ["tick", "errorbar-ticks"],
      "from": {"data": "data_1"},
      "encode": {
        "update": {
          "opacity": {"value": 0.7},
          "fill": {"value": "#4c78a8"},
          "tooltip": {
            "signal": "{\"Year (year)\": timeFormat(datum[\"year_Year\"], ''), \"Miles per Gallon (95% CIs)\": format(datum[\"upper_Miles_per_Gallon\"], \"\")}"
          },
          "xc": {"scale": "x", "field": "year_Year"},
          "yc": {"scale": "y", "field": "upper_Miles_per_Gallon"},
          "width": {"value": 14},
          "height": {"value": 1}
        }
      }
    },
    {
      "name": "layer_2_marks",
      "type": "rule",
      "style": ["rule", "errorbar-rule"],
      "from": {"data": "data_2"},
      "encode": {
        "update": {
          "stroke": {"value": "black"},
          "tooltip": {
            "signal": "{\"Year (year)\": timeFormat(datum[\"year_Year\"], ''), \"Miles per Gallon (95% CIs)\": format(datum[\"lower_Miles_per_Gallon\"], \"\"), \"upper_Miles_per_Gallon\": format(datum[\"upper_Miles_per_Gallon\"], \"\")}"
          },
          "x": {"scale": "x", "field": "year_Year"},
          "y": {"scale": "y", "field": "lower_Miles_per_Gallon"},
          "y2": {"scale": "y", "field": "upper_Miles_per_Gallon"}
        }
      }
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "time",
      "domain": {
        "fields": [
          {"data": "data_0", "field": "year_Year"},
          {"data": "data_1", "field": "year_Year"},
          {"data": "data_2", "field": "year_Year"}
        ]
      },
      "range": [0, {"signal": "width"}]
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "fields": [
          {"data": "data_0", "field": "lower_Miles_per_Gallon"},
          {"data": "data_1", "field": "upper_Miles_per_Gallon"},
          {"data": "data_2", "field": "lower_Miles_per_Gallon"},
          {"data": "data_2", "field": "upper_Miles_per_Gallon"}
        ]
      },
      "range": [{"signal": "height"}, 0],
      "zero": false,
      "nice": true
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "grid": false,
      "title": "Year (year)",
      "labelFlush": true,
      "labelOverlap": true,
      "tickCount": {"signal": "ceil(width/40)"},
      "zindex": 1
    },
    {
      "scale": "x",
      "orient": "bottom",
      "grid": true,
      "gridScale": "y",
      "tickCount": {"signal": "ceil(width/40)"},
      "domain": false,
      "labels": false,
      "maxExtent": 0,
      "minExtent": 0,
      "ticks": false,
      "zindex": 0
    },
    {
      "scale": "y",
      "orient": "left",
      "grid": false,
      "title": "Miles per Gallon (95% CIs)",
      "labelOverlap": true,
      "tickCount": {"signal": "ceil(height/40)"},
      "zindex": 1
    },
    {
      "scale": "y",
      "orient": "left",
      "grid": true,
      "gridScale": "x",
      "tickCount": {"signal": "ceil(height/40)"},
      "domain": false,
      "labels": false,
      "maxExtent": 0,
      "minExtent": 0,
      "ticks": false,
      "zindex": 0
    }
  ],
  "config": {"axisY": {"minExtent": 30}}
}
