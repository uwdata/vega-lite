{
  "$schema": "https://vega.github.io/schema/Vegemite/v2.json",
  "data": {"url": "data/cars.json"},
  "mark": "bar",
  "encoding": {
    "x": {
      "field": "Origin",
      "type": "nominal",
      "scale": {"rangeStep": 25}
    },
    "y": {
      "aggregate": "count",
      "field": "*",
      "type": "quantitative"
    }
  }
}
