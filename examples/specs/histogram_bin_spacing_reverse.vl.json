{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "data": {"url": "data/movies.csv"},
  "mark": {"type": "bar", "binSpacing": 3},
  "encoding": {
    "x": {
      "bin": true,
      "field": "IMDB Rating",
      "type": "quantitative",
      "scale": {"reverse": true}
    },
    "y": {
      "aggregate": "count",
      "type": "quantitative"
    }
  }
}
