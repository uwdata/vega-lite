{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "description": "Stock prices of 5 Tech Companies Over Time.",
  "data": {
    "url": "data/stocks.csv"
  },
  "layer": [
    {
      "mark": "line",
      "encoding": {
        "x": {
          "field": "date",
          "type": "temporal",
          "axis": {
            "format": "%Y"
          }
        },
        "y": {
          "field": "price",
          "type": "quantitative"
        },
        "detail": {
          "field": "symbol",
          "type": "nominal"
        }
      }
    },
    {
      "mark": {
        "type": "point",
        "filled": true,
        "opacity": 0
      },
      "encoding": {
        "x": {
          "field": "date",
          "type": "temporal",
          "axis": {
            "format": "%Y"
          }
        },
        "y": {
          "field": "price",
          "type": "quantitative"
        },
        "detail": {
          "field": "symbol",
          "type": "nominal"
        }
      }
    }
  ]
}