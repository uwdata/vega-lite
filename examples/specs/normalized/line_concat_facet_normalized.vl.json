{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "data": {"url": "data/seattle-weather.csv"},
  "vconcat": [
    {
      "facet": {"row": {"field": "date", "timeUnit": "year"}},
      "spec": {
        "height": 30,
        "mark": "line",
        "encoding": {
          "y": {"field": "precipitation", "aggregate": "mean", "title": null},
          "x": {"field": "date", "timeUnit": "month", "type": "ordinal"}
        }
      }
    },
    {
      "mark": "line",
      "encoding": {
        "x": {"field": "date", "timeUnit": "month", "type": "ordinal"},
        "y": {
          "field": "precipitation",
          "aggregate": "mean",
          "title": "All years"
        }
      },
      "height": 50
    }
  ],
  "config": {"facet": {"spacing": 0}}
}