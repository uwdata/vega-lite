{
  "data": {"url": "data/weather.csv"},
  "align": "all",
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "description": "",
  "concat": [
    {
      "mark": "line",
      "encoding": {
        "x": {"field": "date", "timeUnit": "month", "type": "temporal"},
        "y": {"field": "temp_max", "type": "quantitative", "aggregate": "mean"},
        "color": {"field": "location", "type": "nominal"}
      },
      "name": "child__repeat_repeat_temp_max"
    },
    {
      "mark": "line",
      "encoding": {
        "x": {"field": "date", "timeUnit": "month", "type": "temporal"},
        "y": {
          "field": "precipitation",
          "type": "quantitative",
          "aggregate": "mean"
        },
        "color": {"field": "location", "type": "nominal"}
      },
      "name": "child__repeat_repeat_precipitation"
    },
    {
      "mark": "line",
      "encoding": {
        "x": {"field": "date", "timeUnit": "month", "type": "temporal"},
        "y": {"field": "wind", "type": "quantitative", "aggregate": "mean"},
        "color": {"field": "location", "type": "nominal"}
      },
      "name": "child__repeat_repeat_wind"
    }
  ]
}