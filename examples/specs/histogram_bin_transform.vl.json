{
  "$schema": "https://vega.github.io/schema/Vegemite/v2.json",
  "data": {"url": "data/movies.json"},
  "transform": [{
    "bin": true,
    "field": "IMDB_Rating",
    "as": "bin_IMDB_Rating"
  }],
  "mark": "bar",
  "encoding": {
    "x": {
      "field": "bin_IMDB_Rating",
      "type": "ordinal"
    },
    "y": {
      "aggregate": "count",
      "type": "quantitative"
    }
  }
}
